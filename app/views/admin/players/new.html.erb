<div class ="container">
  <div class ="row justify-content-center">
   <div class ="col-7">
    <h3>演奏家登録</h3>
     <%= form_with model:@player,url:admin_players_path,method: :post do |f| %>
      名前：    <%= f.text_field:name %><br>
      担当楽器：<%= f.select :instrument,Player.instruments.keys.map {|k| [I18n.t("enums.player.instrument.#{k}"), k]} %><br>
      自己紹介：<%= f.text_field :introduction %><br>
      <%= f.submit"登録する",class:"btn btn-success" %>
     <% end %>
   </div>
  </div>
</div>

