<div class ="container">
  <div class ="row justify-content-center">
    <div class ="col-5">
      <%= form_with model: @customer,url: customer_path(@customer.id),method: :patch do |f| %>

         <li>
           <label>姓：</label>
           <%= f.text_field :last_name %>
         </li>

         <li>
           <label>名：</label>
           <%= f.text_field :first_name %>
         </li>

         <li>
           <label>メールアドレス：</label>
           <%= f.text_field :email %>
         </li>

         <li>
           <label>電話番号：</label>
           <%= f.text_field :telephone %>
         </li>

        <%= f.submit"変更する",class:"btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>